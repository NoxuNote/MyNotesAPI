{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "suppressImplicitAnyIndexErrors": true,
      "moduleResolution": "node",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "outDir": "./dist/",
      "sourceMap": true
    },
    "include": [
      "./**/*",
      "./api/openapi.yaml"
    ],
    "exclude": ["node_modules", "**/*.spec.ts"]
    }